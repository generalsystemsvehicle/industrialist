{
  "name": "riverbedlab/industrialist",
  "description": "A SAML2 Service Provide for Laravel 6",
  "license": "MIT",
  "keywords": [
    "saml2",
    "sso",
    "laravel",
    "authentication"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Lee Hilton",
      "email": "lee.hilton@riverbed.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0",
    "onelogin/php-saml": "^3.3.0"
  },
  "require-dev": {
    "nunomaduro/larastan": "^0.4.3",
    "orchestra/testbench": "^4.2",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Riverbedlab\\Industrialist\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Riverbedlab\\Industrialist\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit",
    "larastan": "phpstan analyse --level=max",
    "test": [
        "phpunit",
        "phpstan analyse --level=max"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Riverbedlab\\Industrialist\\Providers\\ServiceProvider"
      ],
      "aliases": {
        "Industrialist": "Riverbedlab\\Industrialist\\Facades\\Industrialist"
      }
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
